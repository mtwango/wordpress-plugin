{
    "name": "mtwango/wordpress-plugin-test",
    "type": "project",
    "authors": [
        {
            "name": "Mtwango",
            "email": "mtwango@gmail.com"
        },
        {
            "name": "Druid.fi",
            "email": "info@druid.fi"
        }
    ],
    "require": {
        "mtwango/wordpress-plugin": "*",
        "johnpbloch/wordpress-core": "^6.2",
        "wpackagist-plugin/akismet": "^5.2"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.packagist.org",
            "exclude": ["mtwango/wordpress-plugin", "wordpress/*"]
        },
        {
            "type": "path",
            "url": ".."
        },
        {
            "type": "composer",
            "url": "https://wpackagist.org"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "cweagans/composer-patches": true,
            "composer/installers": true,
            "mtwango/wordpress-plugin": true
        }
    },
    "extra": {
        "wordpress-plugin": {
            "symlinks": {
                "vendor/wordpress/.htaccess": "public/.htaccess"
            }
        }
    }
}
